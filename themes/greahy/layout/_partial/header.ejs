<header id="header">
  <div id="banner"></div>
  <div id="header-outer" class="outer">
<!--    <div id="header-title" class="inner">-->
<!--      <h1 id="logo-wrap">-->
<!--        <a href="<%- url_for() %>" id="logo"><%= config.title %></a>-->
<!--      </h1>-->
<!--      <% if (theme.subtitle || config.subtitle){ %>-->
<!--        <h2 id="subtitle-wrap">-->
<!--          <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle || config.subtitle %></a>-->
<!--        </h2>-->
<!--      <% } %>-->
<!--    </div>-->
    <div id="header-inner" class="inner">
      <nav id="main-nav">
        <a class="main-nav-link main-nav-root" href="/">Home</a>
        <% if (theme && theme.menu) { %>
          <a id="main-nav-toggle" class="nav-icon"></a>
        <% } %>
        <% for (var i in theme.menu){ %>
          <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
        <% } %>
      </nav>
      <nav id="sub-nav">
        <% if (theme.rss){ %>
          <a id="nav-rss-link" class="nav-icon" href="<%- url_for(theme.rss) %>" title="<%= __('rss_feed') %>"></a>
        <% } %>
        <% if (theme.instagram){ %>
          <a id="nav-instagram-link" href="<%- url_for(theme.instagram) %>" title="<%= __('instagram') %>">
              <img src="/img/instagram.svg" class="nav-icon"  alt="<%= __('instagram') %>" width="20px" height="20px">
          </a>
        <% } %>
        <% if (theme.linkedin){ %>
          <a id="nav-linkedin-link" href="<%- url_for(theme.linkedin) %>" title="<%= __('linkedin') %>">
              <img src="/img/linkedin.svg" class="nav-icon" alt="<%= __('linkedin') %>" width="20px" height="20px">
          </a>
        <% } %>
        <% if (theme.youtube){ %>
          <a id="nav-youtube-link" href="<%- url_for(theme.youtube) %>" title="<%= __('youtube') %>">
              <img src="/img/youtube.svg" class="nav-icon" alt="<%= __('youtube') %>" width="20px" height="20px">
          </a>
        <% } %>
<!--        <a id="nav-search-btn" class="nav-icon" title="<%= __('search') %>"></a>-->
      </nav>
<!--      <div id="search-form-wrap">-->
<!--        <%- search_form({button: '&#xF002;', text: __('search')}) %>-->
<!--      </div>-->
    </div>
  </div>
</header>
